package rules
import com.Q2S.Q2S_Senior_Project.Models.CourseMapping;

rule "Mapping for CSC101"
	when
		productObject: CourseMapping(startCourse=="CSC101")
	then
		productObject.setMapping("CSC1001 CSC1002");
	end
rule "Mapping for CSC202"
	when
		productObject: CourseMapping(startCourse=="CSC202")
	then
		productObject.setMapping("");
	end
